<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>
	<setting type="sep"/>
	<setting id="format" type="enum" label="Audio Quality"
		values="Low|Medium|High" default="2"/>
	<setting type="sep"/>
	<setting id="skin" type="labelenum" label="Skin"
		values="Default|Alaska|Smoke" default="Smoke"/>
	<setting id="thumbnail_artwork" type="enum" label="Thumbnail Artwork"
		values="Album|Artist" default="0"/>
	<setting id="sort_stations" type="enum" label="Sort stations"
        values="By Name|By Country|By City|By Language" default="0"/>
	<setting type="sep"/>
	<setting id="auto_start" type="bool" label="Automatically start playing from previously selected station"
		default="false"/>
	<setting type="sep"/>
	<setting id="firstrun" type="bool" label="" visible="false"
		default="false"/>
	<setting id="last_station_id" type="text" label="" visible="false"
		default="0"/>
</settings>
